/* component/star.wxss */
.wrapper{
  box-sizing: border-box;
  padding-bottom: 20rpx;
}
.star_view{
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
}
.star_view .star{
  width: 20rpx;
  height: 20rpx;
  vertical-align: middle;
}
.star_view .star_wrapper{
  vertical-align: middle;
}
.star_view text{
  vertical-align: middle;
  font-size: 40rpx;
  line-height: 20rpx;
  margin-left: 20rpx;
}